#include<stdio.h>
void Selection_sort(int a[],int n)
{
    int temp;
    for(int i=0;i<n-1;i++)
    {
        for(int j=i+1;j<n;j++)
        {
            if(a[j]<a[i])
            {
                temp = a[j];
                a[j] = a[i];
                a[i] = temp;
            }
        }
    }
    printf("\n");

    for(int i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
}

